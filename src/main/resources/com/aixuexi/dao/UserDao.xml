<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aixuexi.dao.UserDao">
    <select id="getUsers" resultType="com.aixuexi.bean.User">
    select * from tbl_user tu;
  </select>
    <select id="getUserById" resultType="com.aixuexi.bean.User">
        select * from tbl_user where id=#{id};
    </select>


</mapper>